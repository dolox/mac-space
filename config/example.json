{
	"delay": 0.5,

	"space": [{
		"column": {
			"max": 2,
			"spacing": 1
		},

		"row": {
			"max": 2,
			"spacing": 1
		},

		"space": 2,

		"window": [{
			"application": "Finder",
			"description": "Open a Finder window.",
			"osascript": "tell application \"Finder\" to make new Finder window"
		}, {
			"application": "Notes",
			"description": "Open the Notes application."
		}, {
			"application": "TextEdit",
			"description": "Open the TextEdit application with pre-filled text.",
			"shell": "echo 'mac-space' | open -f"
		}, {
			"application": "Terminal",
			"description": "Open the Terminal application and run a command.",
			"osascript": "tell application \"Terminal\" to do script \"ls -la\""
		}]
	}, {
		"column": {
			"max": 0
		},

		"row": {
			"max": 0
		},

		"space": 3,

		"window": [{
			"application": "TextMate",

			"osascriptPre": "tell application \"TextMate\" to close (every window whose name contains \"talklive-sock\")",

			"osascript": [
				"(do shell script \"/usr/local/bin/mate ~/Developer/tristate/talklive-sock\")"
			]
		}]
	}]
}
